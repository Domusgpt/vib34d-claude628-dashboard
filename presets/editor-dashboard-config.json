{
  "editorDashboard": {
    "version": "1.0.0",
    "masterControls": {
      "visualIntensity": {
        "type": "slider",
        "min": 0.1,
        "max": 2.0,
        "default": 0.8,
        "step": 0.1,
        "label": "Master Visual Intensity",
        "cssProperty": "--master-intensity",
        "affects": ["all-visualizers", "all-animations"]
      },
      "gridComplexity": {
        "type": "slider", 
        "min": 1,
        "max": 5,
        "default": 3,
        "step": 1,
        "label": "Grid Layer Complexity",
        "cssProperty": "--grid-layers",
        "affects": ["background-grids", "accent-patterns"]
      },
      "crystallineAccents": {
        "type": "toggle",
        "default": true,
        "label": "Crystal Accent System",
        "cssProperty": "--crystal-accents-enabled",
        "affects": ["bezels", "buttons", "highlights"]
      },
      "colorVibrancy": {
        "type": "slider",
        "min": 0.3,
        "max": 1.5,
        "default": 0.9,
        "step": 0.1,
        "label": "Color Vibrancy",
        "cssProperty": "--color-saturation",
        "affects": ["all-colors", "glow-effects"]
      },
      "animationSpeed": {
        "type": "slider",
        "min": 0.5,
        "max": 3.0,
        "default": 1.0,
        "step": 0.1,
        "label": "Animation Speed Multiplier",
        "cssProperty": "--animation-speed",
        "affects": ["transitions", "rotations", "morphing"]
      }
    },
    "pageRelations": {
      "home": {
        "role": "master",
        "geometry": "hypercube",
        "colorBase": [1.0, 0.0, 1.0],
        "modifiers": {
          "intensityModifier": 1.0,
          "gridModifier": 1.0,
          "speedModifier": 1.0
        }
      },
      "tech": {
        "role": "derivative",
        "geometry": "tetrahedron", 
        "colorBase": [0.0, 1.0, 1.0],
        "modifiers": {
          "intensityModifier": 0.7,
          "gridModifier": 0.6,
          "speedModifier": 1.2
        },
        "relationToMaster": "calculated"
      },
      "media": {
        "role": "derivative",
        "geometry": "sphere",
        "colorBase": [1.0, 1.0, 0.0],
        "modifiers": {
          "intensityModifier": 1.3,
          "gridModifier": 1.4,
          "speedModifier": 0.8
        },
        "relationToMaster": "calculated"
      },
      "audio": {
        "role": "derivative",
        "geometry": "torus",
        "colorBase": [0.0, 1.0, 0.0],
        "modifiers": {
          "intensityModifier": 0.9,
          "gridModifier": 0.9,
          "speedModifier": 1.1
        },
        "relationToMaster": "calculated"
      },
      "quantum": {
        "role": "derivative",
        "geometry": "wave",
        "colorBase": [1.0, 0.0, 0.5],
        "modifiers": {
          "intensityModifier": 1.1,
          "gridModifier": 1.2,
          "speedModifier": 0.9
        },
        "relationToMaster": "calculated"
      }
    },
    "interactionPresets": {
      "cubeNavigation": {
        "enabled": true,
        "dragThreshold": {
          "type": "slider",
          "min": 20,
          "max": 200,
          "default": 80,
          "label": "Drag Sensitivity"
        },
        "tensionBuildup": {
          "type": "slider",
          "min": 0.01,
          "max": 0.1,
          "default": 0.02,
          "label": "Tension Buildup Rate"
        },
        "snapStrength": {
          "type": "slider",
          "min": 0.5,
          "max": 2.0,
          "default": 1.0,
          "label": "Snap Feedback Strength"
        }
      },
      "contentGravity": {
        "enabled": true,
        "gravityStrength": {
          "type": "slider",
          "min": 0.1,
          "max": 1.0,
          "default": 0.3,
          "label": "Content Gravity Strength"
        },
        "proximityRadius": {
          "type": "slider",
          "min": 50,
          "max": 300,
          "default": 150,
          "label": "Gravity Effect Radius"
        }
      },
      "hoverEffects": {
        "enabled": true,
        "enlargementScale": {
          "type": "slider",
          "min": 1.05,
          "max": 1.5,
          "default": 1.15,
          "label": "Hover Scale Factor"
        },
        "glowIntensity": {
          "type": "slider",
          "min": 0.2,
          "max": 1.0,
          "default": 0.6,
          "label": "Hover Glow Intensity"
        }
      },
      "tensionBuild": {
        "enabled": true,
        "affectsState": {
          "parameter": "complexity",
          "operation": "multiply",
          "value": 1.1,
          "duration": 200
        },
        "label": "Cube Drag Start Effect",
        "description": "Visual effect when starting a cube drag."
      },
      "cubeTension": {
        "enabled": true,
        "affectsState": {
          "parameter": "currentDragTension",
          "operation": "set",
          "valueFromIntensity": true
        },
        "label": "Cube Drag Tension Effect",
        "description": "Visual effect proportional to cube drag tension."
      },
      "portalTransitionEffect": {
        "enabled": true,
        "affectsState": {
          "parameter": "intensity",
          "operation": "set",
          "value": 1.5,
          "duration": 800
        },
        "secondaryEffect": {
          "parameter": "complexity",
          "operation": "multiply",
          "value": 1.3,
          "duration": 800
        },
        "label": "Cube Fold Transition Effect",
        "description": "Visual effect when a cube fold transition is committed."
      },
      "visualizerHoverEcosystem": {
        "enabled": true,
        "label": "Visualizer Hover Ecosystem Effect",
        "description": "Coordinated reaction when a visualizer is hovered.",
        "targetEffect": {
          "parameterToChange": "densityMultiplier",
          "value": 0.5, "operation": "multiplyBase",
          "note": "e.g., baseDensity * 0.5. Assumes densityMultiplier is a new property on viz, default 1."
        },
        "observerEffect": {
          "parameterToChange": "densityMultiplier",
          "value": 1.25, "operation": "multiplyBase",
          "note": "e.g., baseDensity * 1.25"
        },
        "animation": {
          "duration": 300,
          "easingFunction": "ease-out"
        }
      },
      "visualizerClickEcosystem": {
        "enabled": true,
        "label": "Visualizer Click Ecosystem Effect",
        "description": "Multi-phase coordinated reaction on visualizer click.",
        "phases": [
          {
            "name": "phase1_Impact",
            "targetEffect": { "parameterToChange": "densityMultiplier", "value": 2.0, "operation": "multiplyBase" },
            "observerEffect": { "parameterToChange": "densityMultiplier", "value": 0.5, "operation": "multiplyBase" },
            "animation": { "duration": 200, "easingFunction": "ease-in" }
          },
          {
            "name": "phase2_Normalization",
            "targetEffect": { "parameterToChange": "densityMultiplier", "value": 1.0, "operation": "multiplyBase" },
            "observerEffect": { "parameterToChange": "densityMultiplier", "value": 1.0, "operation": "multiplyBase" },
            "animation": { "duration": 400, "easingFunction": "ease-out", "delay": 50 }
          }
        ]
      },
      "cardHoverEffects": {
        "enabled": true,
        "label": "Card Hover Effects",
        "description": "Effects applied when a blog card is hovered, affecting card DOM and visualizers.",
        "targetCardDOM": {
          "scale": 1.08,
          "translateZ_px": 30,
          "borderColor": "rgba(0, 255, 255, 0.6)",
          "backdropBlur_px": 30,
          "backdropSaturate_percent": 250,
          "zIndex": 10
        },
        "observerCardsDOM": {
          "scale": 0.95,
          "translateZ_px": -10,
          "opacity": 0.7,
          "backdropBlur_px": 15,
          "backdropSaturate_percent": 120,
          "borderColor": "rgba(255, 255, 255, 0.1)"
        },
        "targetCardVisualizer": {
          "opacity": 0.1
        },
        "boardVisualizerOnCardHover": {
            "opacity": 0.15
        },
        "contentGravityOnHover": {
            "enabled": true,
            "strength": 1.0
        },
        "animation": {
          "duration": 600,
          "easingFunction": "cubic-bezier(0.25, 0.46, 0.45, 0.94)"
        }
      }
    },
    "visualEffects": {
      "portalTransitions": {
        "enabled": true,
        "tearIntensity": {
          "type": "slider",
          "min": 0.3,
          "max": 1.5,
          "default": 0.8,
          "label": "Reality Tear Intensity"
        },
        "morphDuration": {
          "type": "slider",
          "min": 300,
          "max": 1500,
          "default": 800,
          "label": "Morph Duration (ms)"
        }
      },
      "moireGlitchBorders": {
        "enabled": false,
        "frequency": {
          "type": "slider",
          "min": 10,
          "max": 50,
          "default": 25,
          "label": "Interference Frequency"
        },
        "colorShift": {
          "type": "toggle",
          "default": true,
          "label": "RGB Color Shifting"
        }
      },
      "holographicShadows": {
        "enabled": false,
        "depthLayers": {
          "type": "slider",
          "min": 2,
          "max": 6,
          "default": 3,
          "label": "Holographic Depth Layers"
        },
        "parallaxStrength": {
          "type": "slider",
          "min": 0.02,
          "max": 0.2,
          "default": 0.1,
          "label": "Parallax Movement"
        }
      }
    },
    "scrollBehavior": {
      "invisibleScrollbars": {
        "enabled": true,
        "snapToCenterX": {
          "type": "toggle",
          "default": true,
          "label": "Auto-Center Horizontally"
        },
        "springReturn": {
          "type": "slider",
          "min": 0.1,
          "max": 1.0,
          "default": 0.6,
          "label": "Spring Return Strength"
        }
      },
      "dragScrolling": {
        "enabled": true,
        "sensitivity": {
          "type": "slider",
          "min": 0.5,
          "max": 2.0,
          "default": 1.0,
          "label": "Drag Scroll Sensitivity"
        }
      }
    }
  }
}